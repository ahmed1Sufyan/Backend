{
    "version": 2,
    "name": "my-mern-app",
    "builds": [
      {
        "src": "./package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }
  